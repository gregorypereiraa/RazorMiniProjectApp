@page
@model RazorPagesMiniProject.Pages.Adresses

@{
    ViewData["Title"] = "Home page";
}

<!DOCTYPE html>

<html>
<h1>Adress</h1>
<form method="post">
    
    <div class="form-group">
        <label asp-for="Address.State"></label>
        <input type="text" asp-for="Address.State" placeholder="State"/>
    </div>
    
    <div class="form-group">
        <label asp-for="Address.City"></label>
        <input type="text" asp-for="Address.City" placeholder="City"/>
    </div>

    <div class="form-group">
        <label asp-for="Address.StreetAddress"></label>
        <input type="text" asp-for="Address.StreetAddress" placeholder="Street Adress"/>
    </div>


    <button type="submit" class="btn btn-primary">Submit</button>
</form>

</html>